from datetime import datetime

# Given date and time as a string
date_str = "2023-10-02 12:39"

# Parse the given date string into a datetime object
given_date = datetime.strptime(date_str, "%Y-%m-%d %H:%M")

# Current date and time
current_date = datetime.now()

if given_date > current_date:
    print("The given date and time is greater than the current date and time.")
else:
    print("The given date and time is not greater than the current date and time.")
